{
  "routes": [
    {
      "src": "/age-table",
      "dest": "/age-table/index.html"
    },
    {
      "src": "/anniversary",
      "dest": "/anniversary/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
